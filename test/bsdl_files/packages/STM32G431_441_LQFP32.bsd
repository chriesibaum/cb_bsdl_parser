   attribute COMPONENT_CONFORMANCE of STM32G431_441_LQFP32: entity is "STD_1149_1_2001";
   
   attribute PIN_MAP of STM32G431_441_LQFP32 : entity is PHYSICAL_PIN_MAP;
   
