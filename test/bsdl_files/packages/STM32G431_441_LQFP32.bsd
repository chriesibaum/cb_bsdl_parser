            PA12       : inout   bit;
            PB0        : inout   bit;
            PB5        : inout   bit;
            PB6        : inout   bit;
            PB7        : inout   bit;
            PB8        : inout   bit;
            PF0        : inout   bit;
            PF1        : inout   bit;
            VDDA       : linkage bit;
            VDD        : linkage bit_vector(0 to 1);
            VSSA       : linkage bit;
            VSS        : linkage bit_vector(0 to 1)
